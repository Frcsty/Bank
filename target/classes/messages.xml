    <!-- [!] AND symbols (&) are not allowed so use &amp; or § [!]

    List of Color Codes
    &0 -> &amp;0 <-> §0 (Black)
    &1 -> &amp;1 <-> §1 (Dark Blue)
    &2 -> &amp;2 <-> §2 (Dark Green)
    &3 -> &amp;3 <-> §3 (Dark Aqua)
    &4 -> &amp;4 <-> §4 (Dark Red)
    &5 -> &amp;5 <-> §5 (Dark Purple)
    &6 -> &amp;6 <-> §6 (Gold)
    &7 -> &amp;7 <-> §7 (Gray)
    &8 -> &amp;8 <-> §8 (Dark Gray)
    &9 -> &amp;9 <-> §9 (Blue)
    &a -> &amp;a <-> §a (Green)
    &b -> &amp;b <-> §b (Aqua)
    &c -> &amp;c <-> §c (Red)
    &d -> &amp;d <-> §d (Light Purple)
    &e -> &amp;e <-> §e (Yellow)
    &f -> &amp;f <-> §f (White)

    List of Format Codes
    &k -> &amp;k <-> §k (Obfuscated)
    &l -> &amp;l <-> §l (Bold)
    &m -> &amp;m <-> §m (Strikethrough)
    &n -> &amp;n <-> §n (Underline)
    &o -> &amp;o <-> §o (Italic)
    &r -> &amp;r <-> §r (Reset)

    [!] AND symbols (&) are not allowed so use &amp; or § [!] -->

    <!-- Prefix for messages -->
    <prefix>
        <msg lang="EN">§7[§2Bank§7]§f </msg>
        <msg lang="SI">§7[§2Banka§7]§f </msg>
    </prefix>

    <!-- No permission message -->
    <message name="no permission message">
        <msg lang="EN" prefix="true">§cYou don't have permission to use this command!</msg>
        <msg lang="SI" prefix="true">§cNimas dovoljenja za uporabo te komande!</msg>
    </message>

    <message name="Console cannot execute message">
        <msg lang="EN" prefix="true">§cConsole cannot execute this command!</msg>
        <msg lang="SI" prefix="true">§cKonzola nemore izvesti taga ukaza!</msg>
    </message>

    <message name="amount check message">
        <msg lang="EN" prefix="true">§b§l$%amount%</msg>
        <msg lang="SI" prefix="true">§b§l$%amount%</msg>
    </message>

    <message name="amount add message">
        <msg lang="EN" prefix="true">§a§l+$%amount%</msg>
        <msg lang="SI" prefix="true">§a§l+$%amount%</msg>
    </message>

    <message name="invalid amount message">
        <msg lang="EN" prefix="true">§cThis number cannot be used in bank: §6%amount%</msg>
        <msg lang="SI" prefix="true">§cTa številka nesme biti uporabljena v banki: §6%amount%</msg>
    </message>

    <!-- BANK -->

    <message name="you do not have money message">
        <msg lang="EN" prefix="true">§cYou dont have enough money on the account!</msg>
        <msg lang="SI" prefix="true">§cNimas dovolj denarja na računu!</msg>
    </message>

    <message name="amount remove message">
        <msg lang="EN" prefix="true">§c§l-$%amount%</msg>
        <msg lang="SI" prefix="true">§c§l-$%amount%</msg>
    </message>

    <message name="not enough money in bank message">
        <msg lang="EN" prefix="true">§cYou dont have enough money in the bank!</msg>
        <msg lang="SI" prefix="true">§cNimas dovolj denarja v banki!</msg>
    </message>

    <!-- ADMIN BANK -->

    <message name="account does not exists message">
        <msg lang="EN" prefix="true">§cThis player does not have bank account</msg>
        <msg lang="SI" prefix="true">§cTa igralec nima bančnega računa</msg>
    </message>

    <message name="target does not enough money in bank message">
        <msg lang="EN" prefix="true">§cPlayer does't have enough money in the bank!</msg>
        <msg lang="SI" prefix="true">§cIgralec nima dovolj denarja v banki!</msg>
    </message>

    <!-- Help message for /bank command unlimited lines -->
    <!-- START -->
    <message name="help line #bankcommand">
        <msg lang="EN" prefix="true">§7Bank Commands:</msg>
        <msg lang="SI" prefix="true">§7Bančne Komande:</msg>
    </message>

    <message name="help line #bankcommand">
        <msg lang="EN" prefix="true">§e/bank §8>> §eShows amount of money you have in bank</msg>
        <msg lang="SI" prefix="true">§e/bank §8>> §ePokaže koliko denarja imaš v banki</msg>
    </message>

    <message name="help line #bankcommand">
        <msg lang="EN" prefix="true">§e/bank §6+ §7[§6amount§7] §8>> §eAdds money to your bank account</msg>
        <msg lang="SI" prefix="true">§e/bank §6+ §7[§6število§7] §8>> §eDoda denar na tvoj račun</msg>
    </message>

    <message name="help line #bankcommand">
        <msg lang="EN" prefix="true">§e/bank §6- §7[§6amount§7] §8>> §eRemoves money from your bank account</msg>
        <msg lang="SI" prefix="true">§e/bank §6- §7[§6število§7] §8>> §eOdstrani denar iz tvojega/ računa</msg>
    </message>
    <!-- END -->

    <!-- Help message for /adminbank command unlimited lines -->
    <!-- START -->
    <message name="help line #adminbankcommand">
        <msg lang="EN" prefix="true">§7AdminBank Commands:</msg>
        <msg lang="SI" prefix="true">§7AdminBanka Komande:</msg>
    </message>

    <message name="help line #adminbankcommand">
        <msg lang="EN" prefix="true">§e/adminbank §7[§6player§7] §8>> §eShows amount of money player has in bank</msg>
        <msg lang="SI" prefix="true">§e/adminbank §7[§6igralec§7] §8>> §ePokaže koliko denarja ima igralec v banki</msg>
    </message>

    <message name="help line #adminbankcommand">
        <msg lang="EN" prefix="true">§e/adminbank §7[§6player§7] §6+ §7[§6amount§7] §8>> §eAdds money to players bank account</msg>
        <msg lang="SI" prefix="true">§e/adminbank §7[§6igralec§7] §6+ §7[§6število§7] §8>> §eDoda denar v bančni račun od igralca</msg>
    </message>

    <message name="help line #adminbankcommand">
        <msg lang="EN" prefix="true">§e/adminbank §7[§6player§7] §6- §7[§6amount§7] §8>> §eRemoves money from players bank account</msg>
        <msg lang="SI" prefix="true">§e/adminbank §7[§6igralec§7] §6- §7[§6število§7] §8>> §eOdstrani denar iz bančnega računa od igralca</msg>
    </message>
    <!-- END -->

</messages>